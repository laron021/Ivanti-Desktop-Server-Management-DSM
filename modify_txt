$Path = 'C:\Users\Aron\Desktop\LT.txt’
$Path2 = 'C:\Users\Aron\Desktop\LT5.txt’
$str = 'C:\Users\Aron\Desktop\string.txt'
$Content = [System.IO.File]::ReadAllLines($Path)

foreach ($string in (Get-Content $str))
{
$Content = $Content -replace $string,''
}
$Content | Set-Content -Path $Path
@(gc $Path) -match '\S'  | out-file $Path
